intf  power  {
	int  x  0
	int  y  0
	pop  x
	pop  y
	push  1
	push  y
	call  sub
	pop  y
	int  z  x
	while  y  >=  1  {
		push  x
		push  z
	  call  mul
		push  1
		push  y
		call  sub
		pop  y
	}
	return  z
}

intf  square  {
	int  n  0
	pop  n
	push  2
	push  n
	call  power
	pop  n
	return  n
}

intf  cube  {
	int  n  0
	pop  n
	push  3
	push  n
	call  power
	pop  n
	return  n
}